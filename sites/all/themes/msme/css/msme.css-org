header .main_header .nav ul li ul li a {border-right:none; border-bottom:1px solid #fff !important; padding:0px; float:left; width:100%;}
header .main_header .nav ul li ul li:last-child{ border-bottom:none; padding-top:5px;}
header .main_header .nav ul li ul li a::hover {  border-right:none; padding:0px; float:left; width:100%;}
header .main_header .nav ul li:hover > ul{ display:block; padding-bottom:5px;}
