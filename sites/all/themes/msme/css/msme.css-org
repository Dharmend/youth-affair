.middle-content .latest-news .lnews-head .lnews-next,.middle-content .latest-news .lnews-head .lnews-prev{padding: 4px 7px;}


.more-button{  background: none repeat scroll 0 0 #769e23;  border-radius: 1px; color: #fff;  padding: 1px 20px 3px;}
