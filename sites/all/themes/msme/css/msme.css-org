/*Header top*/
header .header_top_nav{ background:#eee; width:100%; float:left;}
header .header_top_nav_left{ width:50%; float:left;}
header .tophead_line1{ margin: 6px 0; float:left;}
header .tophead_line2{padding: 12px 0 0 4px; color:#686868; float:left; }
header .tophead_line3{padding: 13px 0 0 2px; float:left;}
header .header_top_nav_right{width: 33%; float:right; text-align:right;}
header .header_top_nav_right a{ float:left; }
header .header_top_nav_right .skip_cont{ color:#666; margin: 11px 10px 0 0;}
header .header_top_nav_right .tophead_fb{margin: 8px 0px 0; background:url("../image/face_icon.png") no-repeat; height:22px; width:22px;}
header .header_top_nav_right .tophead_tw{margin: 8px 5px 0;background:url("../image/twit_icon.png") no-repeat; height:22px; width:22px; }
header .header_top_nav_right .tophead_line4{font-size:1.5em; color:#fff; background:#444; padding: 3.8px 7px; margin-top: 8px; border-radius:1px;}
header .header_top_nav_right .tophead_line5{font-size:1.5em; color:#686868;  margin: 10px 3px 0 0;}
header .header_top_nav_right .tophead_line6{font-size:1.5em; color:#686868; margin: 10px 10px 0 0;}
header .header_top_nav_right .tophead_line7{font-size:1.5em; color:#686868; margin: 10px 10px 0 0;}
header .header_top_nav_right .tophead_line8{font-size:1.5em; color:#686868; margin: 10px 6px 0 0;}
header .header_top_nav_right select{background:#fff; color: #686868; border:1px solid #ccc; font-size: 1.2em; margin-top: 8px; height: 22px; width:120px; }

/*main header*/
header .main_header{ width:100%; float:left;}
header .main_header .logo {  float: left;  margin: 20px 0;  width:72%;}
header .main_header .logo a {  float: left;   width: 100%;}
header .main_header .logo a img {  float: left; padding: 0 10px 0 0; border-right: 1px solid #ccc;}
header .main_header .logo span {  display: table;  width:70%; }
header  .maintitle{font-size:2em; padding: 15px 0 6px 12px; color:#292929;  }
header  .maintitle_line2{font-size: .6em; color:#666; }
header .main_header .nav_search{width:22%; float:left; background:#e7e7e7; margin-top: 40px;  border-radius: 3px; border:1px solid #d3d3d3; }
header .main_header .nav_search input[ type=search] { width:209px; padding:0 0 0 10px; height:35px; border:none; background:#e7e7e7; color:#717171; font-style:italic;}
header .main_header .nav_search input[ type=submit] { height:35px; width:40px; float:right; background:url("../image/search_icon.jpg") no-repeat scroll 14px 11px #dadada; cursor:pointer; border:none;}
header .main_header .nav_sitemap{width:5.8%; float:left; text-align:right; margin-top:51px;}
header .main_header .nav_sitemap a{ color:#0c71a5; font-size:1.4em;}

