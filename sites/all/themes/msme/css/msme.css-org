.flex-control-paging  a {background: none repeat scroll 0 0 rgba(12, 113, 165, 0.75);
    color: #fff;
    font-size: 2em;
	background:#9933FF;}

ol.flex-control-paging {list-style: outside none none;
    margin: -25px 0 0;
    padding: 0;
    width: 100%;
    z-index: 999999;}

ol.flex-control-paging.h-centered {
    bottom: 0;
    position: absolute;
    text-align: right;
}
ol.flex-control-paging li {
    display: inline;
    border-radius: 50%;
    color: #0f719e;
    font-size: 0;
    margin: 0;
    padding: 5px;
    text-decoration: none;
}
ol.flex-control-paging li a {
    display: inline-block;
	background:none repeat scroll 0 0 #0f719e;
}
