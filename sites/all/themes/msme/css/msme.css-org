        .middle-content .main-link .linkicon1{ background:url(../image/icon2.png) no-repeat; height:35px;}
		.middle-content .main-link .linkicon2{ background:url(../image/icon3.png) no-repeat;height:35px;}
		.middle-content .main-link .linkicon3{ background:url(../image/icon4.png) no-repeat;height:35px;}
		.middle-content .main-link .linkicon4{ background:url(../image/icon5.png) no-repeat;height:35px;}
		.middle-content .main-link .linkicon5{ background:url(../image/icon6.png) no-repeat;height:35px;}
		.middle-content .main-link .linkicon6{ background:url(../image/icon6.png) no-repeat;height:35px;}
.middle-content .main-content{ width:100%; float:left; margin:25px 0;}
		.middle-content .main-content .about-msme{ width:70%; float:left; margin:25px 45px 0 0;}
		.middle-content .main-content .about-msme img{ float:left;}
		.middle-content .main-content .about-msme p{ font-size:1.5em; color:#626262; line-height: 25px;  text-align: justify;}
		.middle-content .main-content .about-msme a{ font-size:1.5em; float:right; margin-top: 10px;}
		.middle-content .main-content .new-msme{ width:26%; float:left;}
		.middle-content .main-content .new-msme h2{ text-align:center; font-size:1.5em;}
		.middle-content .main-content .new-msme ul{border:1px solid #c2c2c2; padding:10px 15px; box-sizing: border-box; margin: 7px 0;}
		.middle-content .main-content .new-msme ul li{border-bottom:1px solid #c2c2c2; margin: 0 0 8px;  padding: 0 0 3px;}
		.middle-content .main-content .new-msme ul li:last-child{ border-bottom:none; margin: 0px;    padding: 0px;}		
		.middle-content .main-content .new-msme a.more-button{ font-size:1.5em; float:right; }
		.middle-content .main-content .new-msme ul li a{font-size:1.5em; color:#626262;  text-align: justify;}
		
