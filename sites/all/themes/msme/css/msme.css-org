.banner-main  .slider-news ul li:nth-of-type(2){margin:0px;}
.banner-main  .slider-news ul li img{ float: left;  margin-top: 10px;}
.banner-main  .slider-news ul li span{ float: left; display:block; width:56%; text-align:center; margin-top:20px;}
.banner-main  .slider-news ul li h2{ color:#2E2E2E; font-size:1.8em;}
.banner-main  .slider-news ul li h3{margin:10px 0 15px 0; color:#4E4E4E; font-size:1.5em;}

